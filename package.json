{
  "name": "le-theme",
  "version": "0.0.6",
  "description": "A react UI theme called `le-theme`",
  "main": "dist/index.js",
  "author": "",
  "scripts": {
    "dev": "webpack-dev-server --hot --history-api-fallback --port 8080 --config ./build/webpack.config.js",
    "ts": "tsc",
    "css": "stylus --use ./node_modules/poststylus --with \"['autoprefixer']\" --import ./node_modules/normalize-styl/index.styl src/index.styl -o dist -c -m",
    "prepublish": "npm run ts && npm run css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnnatarHe/le-theme.git"
  },
  "keywords": [
    "react",
    "le",
    "theme",
    "design",
    "ui"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AnnatarHe/le-theme/issues"
  },
  "homepage": "https://github.com/AnnatarHe/le-theme#readme",
  "devDependencies": {
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "source-map-loader": "^0.1.5",
    "typescript": "^2.1.4",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "@types/react": "0.0.0",
    "@types/react-addons-css-transition-group": "^15.0.1",
    "@types/react-dom": "^0.14.20",
    "@types/react-router": "^2.0.41",
    "autoprefixer": "^6.6.1",
    "css-loader": "^0.26.1",
    "html-webpack-plugin": "^2.26.0",
    "normalize-styl": "^4.1.1",
    "normalize.styl": "^3.0.3",
    "poststylus": "^0.2.3",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-router": "^3.0.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0"
  }
}
